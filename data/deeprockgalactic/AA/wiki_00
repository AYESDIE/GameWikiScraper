47\u0438\u0435 \u0440\u0435\u0441\u0443\u0440\u0441\u043e\u0432, \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0445 \u0434\u043b\u044f \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u0439, \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u043d\u044b\u0445 \u043d\u0438\u0436\u0435.\n\u041e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0446\u0435\u043b\u0438.\n\u042d\u0442\u0438 \u0440\u0435\u0441\u0443\u0440\u0441\u044b \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b \u0434\u043b\u044f \u0437\u0430\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u044f \u0442\u0435\u043a\u0443\u0449\u0435\u0439 \u043c\u0438\u0441\u0441\u0438\u0438.\n\u0418\u043d\u043e\u043f\u043b\u0430\u043d\u0435\u0442\u043d\u043e\u0435 \u044f\u0439\u0446\u043e.\n\u0417\u0435\u043b\u0451\u043d\u043e\u0435 \u044f\u0439\u0446\u043e \u0441 \u0440\u043e\u0437\u043e\u0432\u044b\u043c\u0438 \u043a\u0440\u0430\u044f\u043c\u0438 \u043c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u0432 \u0441\u0442\u0435\u043d\u0430\u0445, \u043f\u043e\u0445\u043e\u0436\u0438\u0445 \u043d\u0430 \u0437\u0430\u043b\u0435\u0436\u0438 \u0440\u0443\u0434. \u041c\u043e\u0436\u043d\u043e \u0437\u0430\u043c\u0435\u0442\u0438\u0442\u044c \u043f\u043e \u043f\u0443\u043b\u044c\u0441\u0438\u0440\u0443\u044e\u0449\u0438\u043c \u043a\u0440\u0430\u0441\u043d\u044b\u043c \u0441\u0444\u0435\u0440\u0430\u043c \u043d\u0430 \u0441\u0442\u0435\u043d\u0435. \u041d\u0430 \u0442\u0435\u043a\u0443\u0449\u0438\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u043c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u0442\u043e\u043b\u044c\u043a\u043e, \u043a\u0430\u043a \u043e\u0441\u043d\u043e\u0432\u043d\u0443\u044e \u0446\u0435\u043b\u044c \u0432 \u043c\u0438\u0441\u0441\u0438\u044f\u0445 \u043f\u043e \u0441\u0431\u043e\u0440\u0443 \u0438, \u043a\u0430\u043a \u0432\u0442\u043e\u0440\u0438\u0447\u043d\u0443\u044e \u0446\u0435\u043b\u044c \u0432 \u043c\u0438\u0441\u0441\u0438\u044f\u0445 \u043f\u043e \u0443\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0438\u044e. \u0418\u0433\u0440\u043e\u043a\u0438, \u043a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u043e, \u0434\u043e\u043b\u0436\u043d\u044b \u0441\u043e\u0431\u0440\u0430\u0442\u044c 4, 6 \u0438\u043b\u0438 8 \u044f\u0438\u0446, \u0447\u0442\u043e\u0431\u044b \u0432\u044b\u043f\u043e\u043b\u043d\u0438\u0442\u044c \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u0443\u044e \u0437\u0430\u0434\u0430\u0447\u0443. \u0418\u043d\u043e\u043f\u043b\u0430\u043d\u0435\u0442\u043d\u044b\u0435 \u044f\u0439\u0446\u0430 \u043d\u0430\u0445\u043e\u0434\u044f\u0442\u0441\u044f \u0432 \u043e\u0440\u0433\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0435 \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u043c\u0438\u0441\u0441\u0438\u0439 \u043f\u043e \u0441\u0431\u043e\u0440\u0443 \u044f\u0438\u0446 \u0438 \u0432\u044b\u0433\u043b\u044f\u0434\u044f\u0442 \u043a\u0430\u043a \u0436\u0451\u043b\u0442\u044b\u0435 \u0441\u0444\u0435\u0440\u044b \u043d\u0430 \u0441\u043a\u0430\u043d\u0435\u0440\u0435 \u043c\u0435\u0441\u0442\u043d\u043e\u0441\u0442\u0438.\n\u0410\u043a\u0432\u0430\u0440\u043a.\n\u0412\u0441\u0442\u0440\u0435\u0447\u0430\u0435\u0442\u0441\u044f \u0438\u0441\u043a\u043b\u044e\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0432 \u043c\u0438\u0441\u0441\u0438\u044f\u0445 \u043f\u043e &lt;a href=\"Missions\"&gt;\u0432\u044b\u0432\u043e\u0437\u0443 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432&lt;/a&gt; \u043a\u0430\u043a \u043e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u0446\u0435\u043b\u044c, \u0430\u043a\u0432\u0430\u0440\u043a \u0438\u043c\u0435\u0435\u0442 \u0444\u043e\u0440\u043c\u0443 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u0433\u043e\u043b\u0443\u0431\u044b\u0445 \u043a\u0440\u0438\u0441\u0442\u0430\u043b\u043b\u043e\u0432, \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u044b\u0445 \u0432 \u0441\u043a\u0430\u043b\u0443. \u041e\u043d \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0435\u043d \u0447\u0435\u0440\u0435\u0437 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u0433\u043e\u043b\u0443\u0431\u044b\u0435 \u043a\u0440\u0438\u0441\u0442\u0430\u043b\u043b\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u043d\u0430 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u0438.\n\u041c\u043e\u0440\u043a\u0438\u0442.\n\u0422\u0451\u043c\u043d\u043e-\u0433\u043e\u043b\u0443\u0431\u043e\u0439-\u0441\u0438\u043d\u0438\u0439 \u043c\u0438\u043d\u0435\u0440\u0430\u043b, \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0438\u0439\u0441\u044f \u0432 \u0436\u0438\u043b\u0430\u0445 \u043d\u0430 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u0438 \u0441\u0442\u0435\u043d\u044b, \u043c\u043e\u0440\u043a\u0438\u0442 - \u043e\u0431\u044b\u0447\u043d\u0430\u044f \u0446\u0435\u043b\u044c \u0448\u0430\u0445\u0442\u0451\u0440\u0441\u043a\u043e\u0439 \u044d\u043a\u0441\u043f\u0435\u0434\u0438\u0446\u0438\u0438, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0432 \u043c\u0438\u0441\u0441\u0438\u044f\u0445 \u043d\u0438\u0437\u043a\u043e\u0433\u043e \u0443\u0440\u043e\u0432\u043d\u044f. \u0418\u0433\u0440\u043e\u043a\u0438, \u043a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u043e, \u0434\u043e\u043b\u0436\u043d\u044b \u0431\u0443\u0434\u0443\u0442 \u0434\u043e\u0431\u044b\u0442\u044c \u043e\u0442 200 \u0434\u043e 400 \u043c\u043e\u0440\u043a\u0438\u0442\u0430, \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0434\u043b\u0438\u043d\u044b \u043c\u0438\u0441\u0441\u0438\u0438; \u043f\u043e\u0441\u043b\u0435 \u0442\u043e\u0433\u043e, \u043a\u0430\u043a \u043a\u0432\u043e\u0442\u0430 \u0431\u044b\u043b\u0430 \u0437\u0430\u043f\u043e\u043b\u043d\u0435\u043d\u0430, &lt;a href=\"M.U.L.E.\"&gt;M.U.L.E.&lt;/a&gt; \u0432\u043a\u043b\u044e\u0447\u0430\u0435\u0442 \u0444\u0443\u043d\u043a\u0446\u0438\u044e \u0432\u043e\u0437\u0432\u0440\u0430\u0442\u0430 \u043d\u0430 \u0441\u0442\u0430\u043d\u0446\u0438\u044e.\n\u0414\u043e\u0431\u044b\u0442\u044b\u0439 \u043c\u043e\u0440\u043a\u0438\u0442 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0435\u0442 \u043f\u0440\u0438\u043c\u0435\u0440\u043d\u043e \u043a\u0443\u0431\u043e\u0432\u0438\u0434\u043d\u0443\u044e \u0444\u043e\u0440\u043c\u0443.\n\u0412\u0442\u043e\u0440\u0438\u0447\u043d\u044b\u0435 \u0446\u0435\u043b\u0438.\n\u042d\u0442\u0438 \u0446\u0435\u043b\u0438 \u043d\u0435 \u043a\u0440\u0438\u0442\u0438\u0447\u043d\u044b \u0434\u043b\u044f \u043c\u0438\u0441\u0441\u0438\u0438, \u043d\u043e \u0434\u0430\u044e\u0442 \u043d\u0435\u043f\u043b\u043e\u0445\u0443\u044e \u043d\u0430\u0433\u0440\u0430\u0434\u0443 \u043f\u0440\u0438 \u0441\u0431\u043e\u0440\u0435 \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u043c\u0438\u0441\u0441\u0438\u0438.\n\u0414\u0438\u0441\u0442\u0440\u0443\u043c.\n\u042f\u0440\u043a\u043e-\u0441\u0438\u043d\u0438\u0439 \u043c\u0435\u0442\u0430\u043b\u043b\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u043c\u0438\u043d\u0435\u0440\u0430\u043b, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043c\u043e\u0436\u043d\u043e \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0438\u0442\u044c \u0432 \u0436\u0438\u043b\u0430\u0445 \u043d\u0430 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u0438 \u0441\u0442\u0435\u043d\u044b, \u0434\u0438\u0441\u0442\u0440\u0443\u043c - \u0432\u0442\u043e\u0440\u0438\u0447\u043d\u0430\u044f \u0446\u0435\u043b\u044c, \u044d\u043a\u0441\u043a\u043b\u044e\u0437\u0438\u0432\u043d\u0430\u044f \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u043b\u044f \u043c\u0438\u0441\u0441\u0438\u0439 \u043f\u043e \u0432\u044b\u0432\u043e\u0437\u0443 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432.\n\u041e\u043a\u0430\u043c\u0435\u043d\u0435\u043b\u043e\u0441\u0442\u0438.\n\u041e\u043a\u0430\u043c\u0435\u043d\u0435\u043b\u043e\u0441\u0442\u044c , \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0432\u0437\u044f\u0442\u0430 \u0441\u043e \u0441\u0442\u0435\u043d\u044b. \u0412\u0442\u043e\u0440\u0438\u0447\u043d\u0430\u044f \u0446\u0435\u043b\u044c \u043e\u0431\u044b\u0447\u043d\u043e \u0442\u0440\u0435\u0431\u0443\u0435\u0442 \u0441\u043e\u0431\u0440\u0430\u0442\u044c 10 \u043e\u043a\u0430\u043c\u0435\u043d\u0435\u043b\u043e\u0441\u0442\u0435\u0439.\n\u0413\u043e\u043b\u043e\u043c\u0438\u0442.\n\u041c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u0432 \u0432\u0438\u0434\u0435 \u0442\u0451\u043c\u043d\u043e-\u0441\u0438\u043d\u0435-\u0444\u0438\u043e\u043b\u0435\u0442\u043e\u0432\u044b\u0445 (\u043f\u043e\u0447\u0442\u0438 \u0447\u0435\u0440\u043d\u044b\u0445) \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u0439 \u0432 \u0442\u0443\u043d\u043d\u0435\u043b\u044f\u0445 \u0438 \u043f\u0435\u0449\u0435\u0440\u0430\u0445, \u0433\u043e\u043b\u043e\u043c\u0438\u0442 - \u0440\u0435\u0441\u0443\u0440\u0441 \u0434\u043b\u044f \u0432\u0442\u043e\u0440\u0438\u0447\u043d\u043e\u0439 \u0446\u0435\u043b\u0438.\n\u0421\u043e\u0437\u0434\u0430\u043d\u0438\u0435/\u0423\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u0435.\n\u042d\u0442\u0438 \u0440\u0435\u0441\u0443\u0440\u0441\u044b \u0440\u0435\u0434\u043a\u043e \u0442\u0440\u0435\u0431\u0443\u044e\u0442\u0441\u044f \u0434\u043b\u044f \u043c\u0438\u0441\u0441\u0438\u0439, \u043d\u043e \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b \u0434\u043b\u044f \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u0439 \u0438 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f \u043a\u043e\u0441\u043c\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u043e\u0432. \u0422\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u043a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u043e, \u0438\u0445 \u0441\u0442\u043e\u0438\u0442 \u0441\u043e\u0431\u0438\u0440\u0430\u0442\u044c \u0432\u0441\u044f\u043a\u0438\u0439 \u0440\u0430\u0437, \u043a\u043e\u0433\u0434\u0430 \u043e\u043d\u0438 \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0438\u0432\u0430\u044e\u0442\u0441\u044f. \u041e\u043d\u0438 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0432 \u0432\u0430\u0448 \u0438\u043d\u0432\u0435\u043d\u0442\u0430\u0440\u044c \u043f\u0440\u0438 \u0437\u0430\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u0438 \u043c\u0438\u0441\u0441\u0438\u0439.\n\u0411\u0438\u0441\u043c\u043e\u0440.\n\u041c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u0432 \u0432\u0438\u0434\u0435 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u0436\u0435\u043b\u0442\u043e\u0432\u0430\u0442\u044b\u0445 \u043a\u0443\u0431\u0438\u043a\u043e\u0432 \u0441 \u0442\u0451\u043c\u043d\u043e-\u043a\u043e\u0440\u0438\u0447\u043d\u0435\u0432\u044b\u043c\u0438 \u0432\u044b\u0434\u0435\u043b\u0435\u043d\u0438\u044f\u043c\u0438, \u0431\u0438\u0441\u043c\u043e\u0440 - \u043a\u043b\u044e\u0447\u0435\u0432\u043e\u0439 \u0440\u0435\u0441\u0443\u0440\u0441 \u0434\u043b\u044f \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f, \u0447\u0430\u0441\u0442\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0439 \u0434\u043b\u044f \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044f \u0432\u0437\u0440\u044b\u0432\u0447\u0430\u0442\u043a\u0438 \u0438 \u0431\u0440\u043e\u043d\u0438.\n\u0411\u0438\u0441\u043c\u043e\u0440 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0432 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430\u0445 \u0432 \u0431\u0438\u043e\u0437\u043e\u043d\u0435 \u0437\u0430\u0440\u043e\u0441\u043b\u0435\u0439 \u0438 \u0432 \u043c\u0430\u043b\u044b\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430\u0445 \u0432 \u0441\u043e\u043b\u044f\u043d\u044b\u0445 \u044f\u043c\u0430\u0445.\n\u041a\u0440\u043e\u043f\u043f\u0430.\n\u0411\u043b\u0435\u0434\u043d\u043e-\u0437\u0435\u043b\u0451\u043d\u0430\u044f \u043c\u0435\u0442\u0430\u043b\u043b\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0440\u0443\u0434\u0430 \u0441 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u043c\u0438 \u0444\u0438\u043e\u043b\u0435\u0442\u043e\u0432\u044b\u043c\u0438 \u0448\u0438\u043f\u0430\u043c\u0438, \u041a\u0440\u043e\u043f\u043f\u0430 \u043c\u043e\u0436\u043d\u043e \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0438\u0442\u044c \u0432 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u044b\u0445 \u043e\u0442\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u0445, \u043f\u043e\u0445\u043e\u0436\u0438\u0445 \u043d\u0430 \u043c\u043e\u0440\u043a\u0438\u0442 \u0438 \u0437\u043e\u043b\u043e\u0442\u043e.\n\u041a\u0440\u043e\u043f\u043f\u0430 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0432 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430\u0445 \u0432 \u0433\u0440\u0438\u0431\u043d\u044b\u0445 \u0442\u043e\u043f\u044f\u0445 \u0438 \u0432 \u043c\u0430\u043b\u044b\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430\u0445 \u0432 \u043c\u0430\u0433\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u044f\u0434\u0440\u0435.\n\u0416\u0435\u043c\u0447\u0443\u0433 \u044d\u043d\u043e\u0440\u0430.\n\u041c\u044f\u0433\u043a\u0438\u0439 \u0441\u0435\u0440\u044b\u0439 \u043c\u0438\u043d\u0435\u0440\u0430\u043b \u0434\u043e\u0441\u0442\u0443\u043f\u0435\u043d \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u0432\u0438\u0434\u0435 \u043a\u0440\u0443\u043f\u043d\u044b\u0445 \u0441\u0444\u0435\u0440\u043e\u0438\u0434\u043e\u0432, \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0438\u0445\u0441\u044f \u0432 \u043b\u0430\u043d\u0434\u0448\u0430\u0444\u0442\u0435, \u0436\u0435\u043c\u0447\u0443\u0433 \u044d\u043d\u043e\u0440\u0430 \u043c\u043e\u0436\u043d\u043e \u